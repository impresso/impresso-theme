    "jest": "*",
    "jest-serializer-vue": "*",
    "jquery": "*",
    "node-notifier": "*",
    "node-sass": "*",
    "optimize-css-assets-webpack-plugin": "*",
    "ora": "*",
    "portfinder": "*",
    "postcss-import": "*",
    "postcss-loader": "*",
    "postcss-url": "*",
    "rimraf": "*",
    "sass-loader": "*",
    "semver": "*",
    "shelljs": "*",
    "uglifyjs-webpack-plugin": "*",
    "url-loader": "*",
    "vue-jest": "*",
    "vue-loader": "*",
    "vue-style-loader": "*",
    "vue-template-compiler": "*",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "*",
